<!--pages/addresslist/addresslist.wxml-->
<view class='con'>
<form bindsubmit='searchProduct'>
<view class='cc' >
<span>收货人</span>
<span><input type='text' placeholder='请输入收货人' name='uname' ></input></span>
</view>
<view class='c1'>
<span>联系电话</span>
<span><input type='text' placeholder='请输入手机号' name='phone' ></input></span>
</view>
<view class='c2'>
<span>所在地区</span>
<span><input type='text' placeholder='输入地区' name='addre' ></input></span>
</view>
<view class='c3'>
<span>详细地址</span>
<span><input type='text' placeholder='请输入详细地址' name='address' ></input></span>
</view>
<button class='c4' form-type='submit'>
<span>保存地址</span>
</button>
</form>
</view>

